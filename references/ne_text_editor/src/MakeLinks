#!/bin/sh
# Script to build links for all the NE source files and
# make files, for use in multi-architecture systems.

ln -s ../src/BaseMake BaseMake
ln -s ../src/LICENCE LICENCE
ln -s ../src/Makefile Makefile
ln -s ../src/OS_Make_DEC_BSDI_gcc OS_Make_BSDI_gcc
ln -s ../src/OS_Make_DEC_OSF1 OS_Make_DEC_OSF1
ln -s ../src/OS_Make_FreeBSD OS_Make_FreeBSD
ln -s ../src/OS_Make_HP-UX.gcc OS_Make_HP-UX.gcc
ln -s ../src/OS_Make_HP-UX.cc OS_Make_HP-UX.cc
ln -s ../src/OS_Make_IRIX4 OS_Make_IRIX4
ln -s ../src/OS_Make_IRIX5 OS_Make_IRIX5
ln -s ../src/OS_Make_Linux.cc   OS_Make_Linux.cc
ln -s ../src/OS_Make_Linux.gcc  OS_Make_Linux.gcc
ln -s ../src/OS_Make_NetBSD OS_Make_NetBSD
ln -s ../src/OS_Make_SunOS4.acc OS_Make_SunOS4.acc
ln -s ../src/OS_Make_SunOS4.gcc OS_Make_SunOS4.gcc
ln -s ../src/OS_Make_SunOS5.cc  OS_Make_SunOS5.cc
ln -s ../src/OS_Make_SunOS5.gcc OS_Make_SunOS5.gcc
ln -s ../src/pcre.c pcre.c
ln -s ../src/pcre.h pcre.h
ln -s ../src/internal.h internal.h
ln -s ../src/chartables.c chartables.c
ln -s ../src/cmdhdr.h cmdhdr.h
ln -s ../src/debug.c debug.c
ln -s ../src/ecmdarg.c ecmdarg.c
ln -s ../src/ecmdcomp.c ecmdcomp.c
ln -s ../src/ecmdsub.c ecmdsub.c
ln -s ../src/ecompR.c ecompR.c
ln -s ../src/ecompP.c ecompP.c
ln -s ../src/ecrash.c ecrash.c
ln -s ../src/ecutcopy.c ecutcopy.c
ln -s ../src/edisplay.c edisplay.c
ln -s ../src/ee1.c ee1.c
ln -s ../src/ee2.c ee2.c
ln -s ../src/ee3.c ee3.c
ln -s ../src/ee4.c ee4.c
ln -s ../src/eerror.c eerror.c
ln -s ../src/efile.c efile.c
ln -s ../src/eglobals.c eglobals.c
ln -s ../src/ehdr.h ehdr.h
ln -s ../src/einit.c einit.c
ln -s ../src/ekey.c ekey.c
ln -s ../src/ekeysub.c ekeysub.c
ln -s ../src/eline.c eline.c
ln -s ../src/elocal.hpux.c elocal.hpux.c
ln -s ../src/elocal.hpux.h elocal.hpux.h
ln -s ../src/elocal.irix5.c elocal.irix5.c
ln -s ../src/elocal.irix5.h elocal.irix5.h
ln -s ../src/elocal.linux.c elocal.linux.c
ln -s ../src/elocal.linux.h elocal.linux.h
ln -s ../src/elocal.osf1.c elocal.osf1.c
ln -s ../src/elocal.osf1.h elocal.osf1.h
ln -s ../src/elocal.sunos4.c elocal.sunos4.c
ln -s ../src/elocal.sunos4.h elocal.sunos4.h
ln -s ../src/elocal.sunos5.c elocal.sunos5.c
ln -s ../src/elocal.sunos5.h elocal.sunos5.h
ln -s ../src/ematch.c ematch.c
ln -s ../src/ematchR.c ematchR.c
ln -s ../src/erdseqs.c erdseqs.c
ln -s ../src/escrnrdl.c escrnrdl.c
ln -s ../src/escrnsub.c escrnsub.c
ln -s ../src/estore.c estore.c
ln -s ../src/eversion.c eversion.c
ln -s ../src/hpuxlocal.h hpuxlocal.h
ln -s ../src/irix4local.h irix4local.h
ln -s ../src/irix5local.h irix5local.h
ln -s ../src/keyhdr.h keyhdr.h
ln -s ../src/linuxlocal.h linuxlocal.h
ln -s ../src/osf1local.h osf1local.h
ln -s ../src/rdargs.c rdargs.c
ln -s ../src/rehdr.h rehdr.h
ln -s ../src/scomhdr.h scomhdr.h
ln -s ../src/scommon.c scommon.c
ln -s ../src/shdr.h shdr.h
ln -s ../src/sunix.c sunix.c
ln -s ../src/structs.h structs.h
ln -s ../src/sunos4local.h sunos4local.h
ln -s ../src/sunos5local.h sunos5local.h
ln -s ../src/sysunix.c sysunix.c
ln -s ../src/unixhdr.h unixhdr.h
